{
  "name": "backend",
  "version": "1.0.0",
  "main": "main.js",
  "author": "MistyBlunch",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src src/main.ts",
    "build": "tsc",
    "exec": "node build/src/main.js",
    "start": "npm run build && npm run exec",
    "prisma:generate": "prisma generate --schema=src/model/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/model/prisma/schema.prisma",
    "graphql:generate": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@types/convict": "^6.1.6",
    "@types/estree": "^1.0.5",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/node": "^20.11.24",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@prisma/client": "5.10.2",
    "bcrypt": "^5.1.1",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  }
}
