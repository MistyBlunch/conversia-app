generator client {
  provider = "prisma-client-js"
  output = "./generated"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id            Int       @id @default(autoincrement())
  name          String
  email         String    @unique
  password      String
  refreshToken  String
  contacts      Contact[]
}

model Contact {
  id        Int       @id @default(autoincrement())
  name      String
  lastName  String
  age       Int
  phone     String
  user      User      @relation(fields: [userId], references: [id])
  userId    Int
}